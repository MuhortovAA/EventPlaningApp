@model IEnumerable<ViewFullEvent>
<div class="list-form container p-0">
    <div class="card">
        <div class="card-header">События ЦБТ</div>
        <div class="card-body">

            <table class="table table-bordered table-striped table-hover">
                <tr>
                    <th class="text-center">Дата</th>
                    <th class="text-center">Событие</th>
                    <th class="text-center"></th>
                </tr>
                @foreach (ViewFullEvent item in Model)
                {
    <tr>
        <td class="text-center">@item.EventCreate</td>
        <td>@item.SubTitle</td>
        <td class="text-center">
            <div>
                <a class="btn btn-outline-info" asp-action="Detailes" asp-route-id="@item.Id" data-toggle="tooltip" data-placement="top" title="Details"><i class="fa fa-info"></i> </a>
            </div>
        </td>

    </tr>}
            </table>
        </div>
    </div>
</div>
